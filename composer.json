{
    "name": "pear/satis",
    "description": "composer.pear.php.net",
    "repositories": [
        {
            "no-api": true,
            "type": "vcs",
            "url": "https://github.com/pear/HTTP_Request2.git"
        },
        {
            "no-api": true,
            "type": "vcs",
            "url": "https://github.com/pear/Net_URL2.git"
        }
    ],
    "require": {
        "composer/satis": "@alpha",
        "pear/http_request2": "*"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Till Klampaeckel",
            "email": "till@php.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "PEAR\\Satis": "src/"
        }
    },
    "include-path": [
        "vendor/pear/http_request2/",
        "vendor/pear/net_url2"
    ],
    "minimum-stability": "dev"
}
