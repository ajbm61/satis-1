{
    "name": "pear/satis",
    "description": "composer.pear.php.net",
    "repositories": [
        {
            "type": "composer",
            "url": "http://pear.imagineeasy.com"
        }
    ],
    "require": {
        "php": "~5.6",
        "aws/aws-sdk-php": "2.6.*",
        "composer/satis": "*",
        "pear/http_request2": "*",
        "symfony/console": "2.5.*",
        "symfony/event-dispatcher": "2.5.*"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Till Klampaeckel",
            "email": "till@php.net"
        }
    ],
    "autoload": {
        "psr-0": {
            "PEAR\\Satis": "src/"
        }
    },
    "include-path": [
        "vendor/pear/http_request2/",
        "vendor/pear/net_url2"
    ],
    "minimum-stability": "dev"
}
